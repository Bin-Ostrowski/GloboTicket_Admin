             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="GloboTicke_Admin3.Views.EventOverviewPage">
    <ContentPage.Content>
        <Grid>
            <ScrollView>
                <CollectionView
                    ItemsSource="{Binding Events}">
                </CollectionView>
            </ScrollView>
        </Grid>
    </ContentPage.Content>
</ContentPage>